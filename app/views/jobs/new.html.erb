<div class="col-md-9 pull-right">
  <h1> 发布工作 </h1>

  <div class="col-md-8">
    <%= simple_form_for [:admin,@job] do |f| %>

    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :wage_lower_bound, :label => "薪资下限" %>
    <%= f.input :wage_upper_bound, :label => "薪资上限" %>
    <%= f.input :contact_email %>
    <%= f.input :is_hidden %>


    <%= f.submit "Submit" %>
    <% end %>

  </div>
</div>
